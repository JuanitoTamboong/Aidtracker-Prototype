{
  "version": 2,
  "builds": [
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.cjs"
    },
    {
      "src": "/police",
      "dest": "/server.cjs",
      "methods": ["GET"]
    },
    {
      "src": "/fire",
      "dest": "/server.cjs",
      "methods": ["GET"]
    },
    {
      "src": "/ambulance",
      "dest": "/server.cjs",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "server.cjs": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}