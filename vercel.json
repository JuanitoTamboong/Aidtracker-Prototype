{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.cjs"
    },
    {
      "src": "/police",
      "dest": "/police.html"
    },
    {
      "src": "/ambulance", 
      "dest": "/ambulance.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}